{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate with react, redux and mdl, build with webpack",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "clean": "rm -rf lib/*",
    "build": "webpack --progress --colors",
    "dist": "webpack -p --progress --colors --config webpack.config.prod.js",
    "watch": "webpack --progress --colors --watch",
    "start": "webpack-dev-server --port 3000 --hot --inline --content-base public/",
    "test": "mocha src/**/*.spec.*",
    "test:travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:browser": "karma start karma.conf.js",
    "flow": "flow check",
    "flow:start": "flow start",
    "lint": "eslint src/**",
    "lint:autofix": "eslint src/** --fix",
    "coverage": "nyc mocha src/**/*.spec.*",
    "translate": "LANG_DIR=src/translations node bin/translate",
    "release:major": "npm version major && git push origin --follow-tags",
    "release:minor": "npm version minor && git push origin --follow-tags",
    "release:patch": "npm version patch && git push origin --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkpoon/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "mdl",
    "material-design-lite",
    "webpack",
    "boilerplate"
  ],
  "author": "K.K. POON <noopkk@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kkpoon/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/kkpoon/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.22.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.19.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-webpack": "^1.7.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "intl": "^1.2.4",
    "lodash.isequal": "^4.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-intl": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "reselect": "^2.5.1"
  }
}
