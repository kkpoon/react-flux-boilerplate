{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate with react and redux, layout with material-design-lite, compile with babel and build with webpack",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "webpack --progress --colors",
    "dist": "NODE_ENV=production webpack -p --progress --colors",
    "watch": "webpack --progress --colors --watch",
    "server": "node index.js",
    "start": "concurrent \"npm run watch\" \"npm run server\" ",
    "test": "BABEL_JEST_STAGE=0 jest",
    "mocha": "mocha --compilers js:babel-register,jsx:babel-register,coffee:coffee-script/register,ts:espower-typescript/guess,tsx:espower-typescript/guess src/**/*.spec.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkpoon/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "K.K. POON <noopkk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkpoon/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/kkpoon/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.1.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "empty": "^0.10.1",
    "espower-typescript": "^4.0.0",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "mocha": "^2.4.5",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "express": "^4.13.4",
    "falcor": "^0.1.16",
    "falcor-express": "^0.1.2",
    "falcor-http-datasource": "^0.1.3",
    "falcor-router": "^0.2.12",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "material-colors": "^1.0.0",
    "material-design-lite": "^1.0.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "request": "^2.69.0",
    "rx": "^4.0.7",
    "webfontloader": "^1.6.16"
  },
  "jest": {
    "collectCoverage": true,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
