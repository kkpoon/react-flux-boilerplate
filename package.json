{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate with react, redux and mdl, build with webpack",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "webpack --progress --colors",
    "dist": "webpack -p --progress --colors --config webpack.config.prod.js",
    "watch": "webpack --progress --colors --watch",
    "server": "node bin/app",
    "start": "concurrent \"npm run watch\" \"npm run server\" ",
    "test": "mocha src/**/*.spec.*",
    "test:browser": "karma start karma.conf.js",
    "release:major": "npm version major && git push origin --follow-tags",
    "release:minor": "npm version minor && git push origin --follow-tags",
    "release:patch": "npm version patch && git push origin --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkpoon/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "K.K. POON <noopkk@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kkpoon/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/kkpoon/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.1.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "espower-typescript": "^4.0.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "karma": "^0.13.21",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-node-externals": "^0.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "color": "^0.11.1",
    "express": "^4.13.4",
    "express-graphql": "^0.4.9",
    "falcor": "^0.1.16",
    "falcor-express": "^0.1.2",
    "falcor-http-datasource": "^0.1.3",
    "falcor-router": "^0.2.12",
    "graphql": "^0.4.17",
    "immutable": "^3.7.6",
    "intl": "^1.1.0",
    "lodash.isequal": "^4.1.0",
    "material-colors": "^1.0.0",
    "material-design-lite": "^1.1.1",
    "morgan": "^1.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-intl": "^2.0.0-pr-3",
    "react-mdl": "^1.4.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "request": "^2.69.0",
    "reselect": "^2.0.3",
    "rx": "^4.0.7",
    "webfontloader": "^1.6.16"
  }
}
