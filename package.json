{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate with react and redux, layout with material-design-lite, compile with babel and build with webpack",
  "private": true,
  "scripts": {
    "build": "webpack --progress --colors",
    "dist": "webpack -p --progress --colors",
    "watch": "webpack --progress --colors --watch",
    "lite": "lite-server --baseDir ./public",
    "start": "concurrent \"npm run watch\" \"npm run lite\" ",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkpoon/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "K.K. POON <noopkk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkpoon/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/kkpoon/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "lite-server": "^1.3.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "history": "^1.17.0",
    "material-design-lite": "^1.0.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "webfontloader": "^1.6.16"
  }
}
